clear

file = fopen('train_data/training_index_word2vec200.csv');
fseek(file, 0,'eof');
fileSize = ftell(file);
% mat = zeros(fileSize,1);
data = cell(fileSize,1);

file = fopen('train_data/training_index_word2vec200.csv');
i=1;
% while ~feof(file)
endCount=3;
for i=1:endCount
    text = fgetl(file);
    strVec = strsplit(text,',');
    mat = str2double(strVec);
    data(i) = mat2cell(mat);
    progress = i/endCount;
    msg = ['Loading...... ',progress];
    disp(msg)
    i = i + 1;

end


